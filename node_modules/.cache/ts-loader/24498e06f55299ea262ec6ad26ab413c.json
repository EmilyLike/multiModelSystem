AAA,kBAuC4C;AAvC5C;AAAA;AAAA,mBAuC+D,KAAK;AAvCpE;AAAA;AAAA,iBA2Ca;AA3Cb;AAgDD;AAlDH;AAAA;AAAA,WAwGE,mBAAO;AACL,MAAA,OAAO,CAAC,GAAR,CAAY,aAAZ;AACD;AA1GH;AAAA;AAAA,WA4GE,wBAAY;AACV,WAAK,IAAL,CAAU,IAAV,GAAiB,KAAjB;AACD;AA9GH;AAAA;AAAA,WAgHE,oBAAQ;AAAA;;AACL,WAAK,KAAL,CAAW,OAAX,CAA2B,aAA3B,CAAyC,aAAzC,EAAwD,UAAC,YAAD,EAAsB;AAC7E,YAAI,CAAC,YAAL,EAAmB;AACjB;AACA;AACA;AACA;AACA,cAAM,KAAK,GAAG,WAAW,CAAC,YAAK;AAC7B,YAAA,MAAI,CAAC,IAAL,CAAU,IAAV,IAAkB,CAAlB;AACA,YAAA,MAAI,CAAC,IAAL,CAAU,OAAV,aAAuB,MAAI,CAAC,IAAL,CAAU,IAAjC;AACA,YAAA,MAAI,CAAC,IAAL,CAAU,QAAV,GAAqB,IAArB;;AACA,gBAAI,MAAI,CAAC,IAAL,CAAU,IAAV,KAAmB,CAAvB,EAA0B;AACxB,cAAA,MAAI,CAAC,IAAL,CAAU,QAAV,GAAqB,KAArB;AACA,cAAA,MAAI,CAAC,IAAL,CAAU,OAAV,GAAoB,MAApB;AACA,cAAA,MAAI,CAAC,IAAL,CAAU,IAAV,GAAiB,EAAjB;AACA,cAAA,aAAa,CAAC,KAAD,CAAb;AACD;AACF,WAVwB,EAUtB,IAVsB,CAAzB;;AAWA,UAAA,MAAI,CAAC,QAAL,CAAc,OAAd,CAAsB,SAAtB;;AACA,UAAA,MAAI,CAAC,YAAL;AACD;AACF,OApBA;AAqBF;AAtIH;;AAAA;AAAA,EAAmC,GAAnC,CAAA;;AAAqB,KAAK,GAAA,UAAA,CAAA,CADzB,SACyB,CAAA,EAAL,KAAK,CAAL;eAAA,K","sourcesContent":["import { Component, Vue } from 'vue-property-decorator';\nimport './login.scss';\n\n@Component\nexport default class Login extends Vue {\n  render() {\n    return (<div class=\"login\">\n      <div class=\"container\">\n        <div class=\"system-intro\">\n          <div class=\"title\">\n              <i class=\"el-icon-s-help\" style=\"font-size:30px;margin-right:10px;\"></i>\n              <span>  多 模 态 数 据 采 集 系 统 </span>\n          </div>\n          <div class=\"sub-title\">\n            赋能于 ASR 实验室的多模态数据采集、管理一体化服务系统\n          </div>\n        </div>\n        <el-form class=\"system-input\" ref=\"formRef\" rules={this.rules} props={{ model: this.form }} status-icon>\n          <el-form-item prop=\"phoneNumber\">\n            <el-input\n              vModel={this.form.phoneNumber}\n              placeholder=\"请输入手机号\"\n              prefix-icon=\"el-icon-user-solid\"\n            >\n            </el-input>\n          </el-form-item>\n          <el-form-item prop=\"veriCode\">\n            <el-input\n              style=\"width:65%; margin-right:1%;\"\n              type=\"password\"\n              vModel={this.form.veriCode}\n              placeholder=\"请输入验证码\"\n              prefix-icon=\"el-icon-phone\">\n            </el-input>\n            <el-button\n              style=\"width:34%;color:#808080;\"\n              onClick={this.sendCode}\n              disabled={this.form.disabled}\n              >\n              {this.form.btnText}\n            </el-button>\n          </el-form-item>\n        </el-form>\n        <div class=\"system-button\">\n          <el-row type=\"flex\" justify=\"center\" >\n            <el-button style=\"width: 100%\" type=\"primary\" onClick={this.submit}>登录</el-button>\n          </el-row>\n        </div>\n      </div>\n      <div class=\"footer\">\n        copyright© 2021 北京林业大学人工智能研究所出品\n      </div>\n    </div>\n    );\n  }\n\n  form = {\n    phoneNumber: '',\n    veriCode: '',\n    code: '',\n    time: 60,\n    btnText: '发送验证码',\n    disabled: false,\n  };\n\n  validateNumber = (rule: any, value: string, callback: (arg0?: Error | undefined) => void) => {\n    const reg = /^(13[0-9]|14[01456879]|15[0-35-9]|16[2567]|17[0-8]|18[0-9]|19[0-35-9])\\d{8}$/;\n    if (!value) callback(new Error('请输入手机号'));\n    if (!value.match(reg)) {\n      callback(new Error('请输入正确的手机号码'));\n    } else {\n      callback();\n    }\n  };\n\n  validCode = (rule: any, value: string, callback: (arg0?: Error | undefined) => void) => {\n    if (!value) callback(new Error('请输入验证码'));\n    if (value !== this.form.code) {\n      callback(new Error('请输入正确的验证码'));\n    } else {\n      callback();\n    }\n  };\n\n  rules = {\n    phoneNumber: [{\n      required: true, trigger: 'blur', validator: this.validateNumber,\n    }],\n    veriCode: [{ required: true, validator: this.validCode, trigger: 'blur' }],\n  };\n\n  submit = () => {\n    console.log(this.$refs.formRef);\n    (this.$refs.formRef as any).validate(async (valid: any) => {\n      if (!valid) {\n        return;\n      }\n      await this.$store.dispatch('EDIT_USER_INFO', this.form.phoneNumber);\n\n      this.$router.replace('/record').catch((err) => {\n        console.log(err);\n      });\n      // this.$router.push({\n      //   path: '/record',\n      // });\n    });\n  };\n\n  created() {\n    console.log('this is cre');\n  }\n\n  getValidCode() {\n    this.form.code = '123';\n  }\n\n  sendCode() {\n    (this.$refs.formRef as any).validateField('phoneNumber', (errorMessage: any) => {\n      if (!errorMessage) {\n        // 1.时间开始倒数\n        // 2.按钮进入禁用状态\n        // 3.如果倒计时结束 按钮恢复可用状态 按钮文字变为重新发送, 时间重置\n        // 4.倒计时的过程中 按钮文字为 多少s后重新发送\n        const timer = setInterval(() => {\n          this.form.time -= 1;\n          this.form.btnText = `${this.form.time}s后重新发送`;\n          this.form.disabled = true;\n          if (this.form.time === 0) {\n            this.form.disabled = false;\n            this.form.btnText = '重新发送';\n            this.form.time = 60;\n            clearInterval(timer);\n          }\n        }, 1000);\n        this.$message.success('发送验证码成功');\n        this.getValidCode();\n      }\n    });\n  }\n}\n"],"sourceRoot":""}]}